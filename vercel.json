{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/lts/:path*", "destination": "/html/project/:path*" },
    { "source": "/projects/:path*", "destination": "/projects/:path*" },
    { "source": "/:path*", "destination": "/index.js" }
  ]
}
